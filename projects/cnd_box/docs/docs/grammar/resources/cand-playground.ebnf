<KEYWORD_CONTINUE> ::= "continue"

<KEYWORD_TRUE> ::= "true"
<KEYWORD_FALSE> ::= "false"
<KEYWORD_NONE> ::= "none"

<IDENTIFIER> ::= <ALPHA> <ALPHA_DIGIT_LOW_LINE>*
<NUMBER> ::= <DIGIT>+
<REAL> ::= <NUMBER> <FULL_STOP> <NUMBER>
<UNSIGNED_LITERAL> ::= <NUMBER> <SMALL_LETTER_U>
<BIT_LITERAL> ::= <DIGIT_ZERO> <CAPITAL_LETTER_B> | <DIGIT_ONE> <CAPITAL_LETTER_B>
<BYTE_LITERAL> ::= <NUMBER> <SMALL_LETTER_B> | <NUMBER> <SMALL_LETTER_B>
<BOOL_LITERAL> ::= <KEYWORD_TRUE> | <KEYWORD_FALSE>

<escaped_s_char> ::= <REVERSE_SOLIDUS> (<SMALL_LETTER_N> | <SMALL_LETTER_T> | <SMALL_LETTER_R> | <DIGIT_ZERO> | <REVERSE_SOLIDUS> | <QUOTATION_MARK>)
<s_char_sequence> ::= (<escaped_s_char> | <ALPHA> | <DIGIT> | <WHITESPACE>
		   | <EXCLAMATION_MARK> | <NUMBER_SIGN> | <DOLLAR_SIGN> | <PERCENT_SIGN> | <AMPERSAND> | <APOSTROPHE>
		   | <LEFT_PARENTHESIS> | <RIGHT_PARENTHESIS> | <ASTERISK> | <PLUS_SIGN> | <COMMA> | <HYPHEN_MINUS> | <FULL_STOP>
		   | <COLON> | <SEMICOLON> | <LESS_THAN_SIGN> | <EQUALS_SIGN> | <GREATER_THAN_SIGN> | <QUESTION_MARK> | <AT_SIGN> | <LEFT_SQUARE_BRACKET>
		   | <RIGHT_SQUARE_BRACKET> | <CIRCUMFLEX_ACCENT> | <LOW_LINE> | <BACKTICK> | <LEFT_CURLY_BRACKET> | <VERTICAL_LINE>
		   | <RIGHT_CURLY_BRACKET> | <TILDE>)*
<STRING_LITERAL> ::= <QUOTATION_MARK> <s_char_sequence> <QUOTATION_MARK>

<escaped_c_char> ::= <REVERSE_SOLIDUS> (<SMALL_LETTER_N> | <SMALL_LETTER_T> | <SMALL_LETTER_R> | <DIGIT_ZERO> | <REVERSE_SOLIDUS> | <APOSTROPHE> )
<c_char> ::= (<escaped_c_char> | <ALPHA> | <DIGIT> | <WHITESPACE>
		   | <EXCLAMATION_MARK> | <NUMBER_SIGN> | <DOLLAR_SIGN> | <PERCENT_SIGN> | <AMPERSAND> | <QUOTATION_MARK>
		   | <LEFT_PARENTHESIS> | <RIGHT_PARENTHESIS> | <ASTERISK> | <PLUS_SIGN> | <COMMA> | <HYPHEN_MINUS> | <FULL_STOP>
		   | <COLON> | <SEMICOLON> | <LESS_THAN_SIGN> | <EQUALS_SIGN> | <GREATER_THAN_SIGN> | <QUESTION_MARK> | <AT_SIGN> | <LEFT_SQUARE_BRACKET>
		   | <RIGHT_SQUARE_BRACKET> | <CIRCUMFLEX_ACCENT> | <LOW_LINE> | <BACKTICK> | <LEFT_CURLY_BRACKET> | <VERTICAL_LINE>
		   | <RIGHT_CURLY_BRACKET> | <TILDE>)
<CHAR_LITERAL> ::= <APOSTROPHE> <c_char> <APOSTROPHE>

<r_char_sequence> ::= (<ALPHA> | <DIGIT> | <WHITESPACE> | <PUNCTUATOR> | <NEWLINE>)*
<RAW_STRING_LITERAL> ::= <CAPITAL_LETTER_R> <QUOTATION_MARK> <LEFT_PARENTHESIS> <r_char_sequence> <RIGHT_PARENTHESIS> <QUOTATION_MARK>

<lc_char_sequence> ::= (<ALPHA> | <DIGIT> | <WHITESPACE> | <PUNCTUATOR>)*
<LINE_COMMENT> ::= <BACKTICK> <lc_char_sequence>

<bc_char_sequence> ::= (<ALPHA> | <DIGIT> | <WHITESPACE> | <PUNCTUATOR>)*
<BLOCK_COMMENT> ::= <REVERSE_SOLIDUS> <BACKTICK> <bc_char_sequence> <BACKTICK> <REVERSE_SOLIDUS>


<syntax> ::= <pragmatic_stmt>* | <pragmatic_block>
<pragmatic_block> ::= <LEFT_CURLY_BRACKET> <IGNORED> <pragmatic_stmt_list> <IGNORED> <RIGHT_CURLY_BRACKET>
<pragmatic_stmt_list> ::= <pragmatic_stmt>*
<pragmatic_stmt> ::= <primary_stmt> | <process>


<primary_stmt> ::= <primary_expr> <SEMICOLON>

<primary_expr> ::= <LEFT_PARENTHESIS> <primary_expr> <RIGHT_PARENTHESIS> | <list_fold>

<list_fold> ::= <assignment> (<COMMA> <assignment>)*

<assignment> ::= (<logical_or> <EQUALS_SIGN>)* <logical_or>

<logical_or> ::= <logical_and> (<LOGICAL_OR_OPERATOR> <logical_and>)*

<logical_and> ::= <bitwise_or> (<LOGICAL_AND_OPERATOR> <bitwise_or>)*

<bitwise_or> ::= <bitwise_xor> (<VERTICAL_LINE> <bitwise_xor>)*

<bitwise_xor> ::= <bitwise_and> (<CIRCUMFLEX_ACCENT> <bitwise_and>)*

<bitwise_and> ::= <equality> (<AMPERSAND> <equality>)*

<equality> ::= <relational> (<EQUALITY_OPERATOR> <relational>)*
			  | <relational> (<NOT_EQUAL_OPERATOR> <relational>)*

<relational> ::= <three_way_equality> (<GREATER_THAN_SIGN> <three_way_equality>)*
			  | <three_way_equality> (<LESS_THAN_SIGN> <three_way_equality>)*
			  | <three_way_equality> (<GREATER_THAN_EQUAL_SIGN> <three_way_equality>)*
			  | <three_way_equality> (<LESS_THAN_EQUAL_SIGN> <three_way_equality>)*

<three_way_equality> ::= <bitwise_shift> (<SPACESHIP_OPERATOR> <bitwise_shift>)*

<bitwise_shift> ::= <summation> (<BITWISE_RIGHT_SHIFT> <summation>)*
			  	  | <summation> (<BITWISE_LEFT_SHIFT> <summation>)*

<summation> ::= <production> (<PLUS_SIGN> <production>)*
			  | <production> (<HYPHEN_MINUS> <production>)*

<production> ::= <prefix> (<ASTERISK> <prefix>)*
				| <prefix> (<SOLIDUS> <prefix>)*
				| <prefix> (<PERCENT_SIGN> <prefix>)*

<prefix> ::= <INCREMENT_SIGN>* <access>
			| <DECREMENT_SIGN>* <access>
            | <EXCLAMATION_MARK>* <access>

<access> ::= <postfix> (<FULL_STOP> <postfix>)*

<postfix> ::= <resolution> <INCREMENT_SIGN>*
			 | <resolution> <DECREMENT_SIGN>*

<resolution> ::= <operand> (<DOUBLE_COLON> <operand>)*

<operand> ::= <identity_operand> | <value_operand>

<identity_operand> ::= <IDENTIFIER>

<value_operand> ::= <NUMBER> | <REAL> | <UNSIGNED_LITERAL> | <BIT_LITERAL> | <BYTE_LITERAL> | <BOOL_LITERAL>
			| <STRING_LITERAL> | <CHAR_LITERAL> | <RAW_STRING_LITERAL>

<process> ::= <process_decl> | <process_def>
<process_decl> ::= <KEYWORD_PROC> <IGNORED> <AT_SIGN> <IDENTIFIER> <IGNORED> <SEMICOLON>
<process_def> ::= <KEYWORD_PROC> <IGNORED> <AT_SIGN> <IDENTIFIER> <IGNORED> <COLON> <IGNORED> <process_block>
<process_block> ::= <LEFT_CURLY_BRACKET> <IGNORED> <process_stmt_list> <IGNORED> <RIGHT_CURLY_BRACKET>
<process_stmt_list> ::= <process_stmt>*
<process_stmt> ::= <KEYWORD_PROC> <SEMICOLON>

<library> ::= <library_decl> | <library_def>
<library_decl> ::= <KEYWORD_LIB> <IGNORED> <AT_SIGN> <IDENTIFIER> <IGNORED> <SEMICOLON>
<library_def> ::= <KEYWORD_LIB> <IGNORED> <AT_SIGN> <IDENTIFIER> <IGNORED> <COLON> <IGNORED> <library_block>
<library_block> ::= <LEFT_CURLY_BRACKET> <IGNORED> <library_stmt_list> <IGNORED> <RIGHT_CURLY_BRACKET>
<library_stmt_list> ::= <library_stmt>*
<library_stmt> ::= <KEYWORD_LIB> <SEMICOLON>

<include> ::= <KEYWORD_INCLUDE> <WHITESPACE>+ <STRING_LITERAL> <IGNORED> <SEMICOLON>